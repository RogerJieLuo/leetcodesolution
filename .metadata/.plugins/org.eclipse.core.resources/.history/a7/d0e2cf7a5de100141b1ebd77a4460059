package atio;

public class atio {
	public static void main(String [ ] args){
		
	}
	
	public int atio(String str){
		
		str=str.trim();
		String str2int="";
		if(str==""){
			return 0;
		}else{
			for(int i=0;i<str.length();i++){
				if(str.charAt(0)=='-'){
					str2int+="-";
				}else if(Character.isLetter(str.charAt(i))){
					break;
				}else{
					str2int+=str.charAt(i)+"";
				}
			}
		}
		return Integer.parseInt(str2int);
	}
}
