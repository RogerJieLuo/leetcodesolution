package sort;

public class insert {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int score[] ={69, 75, 100, 87, 67,  90, 89, 99, 1, 13};
		//int sorts[]=new int[score.length];
		for(int i=0;i<score.length-1;i++){
			
			for(int j=0;j<i+1;j++){
				//get the part of array that rearrange.
				int count=0;
				int check=i+1;
				while(j<check){
					if(score[j]>score[j+1]){
						int temp=score[j];
						score[j]=score[j+1];
						score[j+1]=temp;
						count++;
					}
					check--;
				}
				System.out.print((check)+" "+count+" : ");
			}
			for(int l=0;l<score.length;l++){
				System.out.print(score[l]+" ");
			}
			System.out.println("");
		}
		
		System.out.println("");
		for(int i=0;i<score.length;i++){
			System.out.print(score[i]+" ");
		}
	}
}
