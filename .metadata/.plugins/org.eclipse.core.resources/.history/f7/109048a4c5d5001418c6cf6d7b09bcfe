package sort;

public class quick {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}
	
	public static void quick(int arr[], int left[], int right[]){
		
		int cur=arr[0];
		int lcount=0;
		int rcount=0;
		for(int i=0;i<arr.length;i++){
			if(arr[i]<cur){
				left[lcount]=arr[i];
				lcount++;
			}else{
				right[rcount]=arr[i];
				rcount++;
			}
		}
		quick(arr,left,right);
	}

}
