package atio;

public class atio {
	public static void main(String [ ] args){
		
	}
	
	public int atio(String str){
		
		str=str.trim();
		String[] each=new String[str.length()];
		if(str==""){
			return 0;
		}else{
			for(int i=0;i<str.length();i++){
				if(str.charAt(0)=='-'){
					each[0]="-";
				}else if(Character.isLetter(str.charAt(i))){
					break;
				}else{
					each[i]=str.charAt(i).toString();
				}	
				
			}
		}
	}
}
