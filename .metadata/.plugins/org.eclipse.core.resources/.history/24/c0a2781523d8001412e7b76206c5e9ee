package discount;

public class DiscountCashBack extends Discount{

	private double total=0;
	public DiscountCashBack(double t, double limit, double cashback){
		if(t>limit){
			total=t-cashback;
		}else{
			total=t;
		}
	}
	
	public double getTotal(double total,double limit, double cashback){
		double result=0;
		
		return result;
	}
}
